<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import java.lang.Double?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Polygon?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="800.0" prefWidth="1200.0" stylesheets="@mainStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.D21.teamE.views.mapEditorControllers.EdgeMapEditor">
    <children>
        <AnchorPane id="app-bar-top">
            <children>
                <VBox id="app-bar-top" layoutX="-10.0" layoutY="-10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <children>
                        <HBox id="app-bar-top-upper" maxHeight="25.0" minHeight="25.0" prefHeight="25.0" stylesheets="@mainStyle.css">
                            <children>
                                <FlowPane id="app-bar-top-upper" alignment="CENTER_RIGHT" columnHalignment="RIGHT" HBox.hgrow="ALWAYS">
                                    <children>
                                        <Rectangle fx:id="fullscreen" arcHeight="5.0" arcWidth="5.0" height="13.0" stroke="#00000004" strokeType="INSIDE" styleClass="app-bar-button" width="13.0" />
                                        <Circle fx:id="hide" radius="7.0" stroke="#00000004" strokeType="INSIDE" styleClass="app-bar-button">
                                            <FlowPane.margin>
                                                <Insets bottom="5.0" left="10.0" top="5.0" />
                                            </FlowPane.margin>
                                        </Circle>
                                        <Polygon fx:id="exit" rotate="180.0" stroke="#00000001" strokeType="INSIDE" styleClass="app-bar-button">
                                            <points>
                                                <Double fx:value="-7.0" />
                                                <Double fx:value="7.0" />
                                                <Double fx:value="7.0" />
                                                <Double fx:value="7.0" />
                                                <Double fx:value="0.0" />
                                                <Double fx:value="-7.0" />
                                            </points>
                                            <FlowPane.margin>
                                                <Insets left="10.0" />
                                            </FlowPane.margin>
                                        </Polygon>
                                    </children>
                                    <padding>
                                        <Insets right="10.0" />
                                    </padding>
                                </FlowPane>
                            </children>
                        </HBox>
                        <HBox id="app-bar-top-main" maxHeight="40.0" minHeight="40.0" prefHeight="40.0">
                            <children>
                                <FlowPane style="-fx-background-color: -fx--primary;" HBox.hgrow="ALWAYS">
                                    <children>
                                        <Label prefHeight="38.0" prefWidth="486.0" text="Edge Map Editor" textFill="WHITE">
                                            <font>
                                                <Font size="20.0" />
                                            </font>
                                            <FlowPane.margin>
                                                <Insets left="10.0" />
                                            </FlowPane.margin>
                                        </Label>
                                    </children>
                                </FlowPane>
                                <FlowPane alignment="CENTER_RIGHT" columnHalignment="RIGHT" style="-fx-background-color: -fx--primary;" HBox.hgrow="ALWAYS">
                                    <padding>
                                        <Insets right="30.0" />
                                    </padding>
                                    <children>
                                        <JFXButton onAction="#getHelpDefault" style="-fx-background-color: -fx--primary-dark;" text="Help" textFill="WHITE" />
                                    </children>
                                </FlowPane>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </AnchorPane>
        <AnchorPane prefHeight="680.0" prefWidth="1200.0">
            <children>
                <HBox prefHeight="610.0" prefWidth="519.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <children>
                        <VBox prefHeight="777.0" prefWidth="1200.0">
                            <children>
                                <TreeTableView fx:id="treeTable" prefHeight="200.0" prefWidth="341.0">
                                    <VBox.margin>
                                        <Insets left="20.0" right="20.0" top="20.0" />
                                    </VBox.margin>
                                </TreeTableView>
                                <HBox>
                                    <children>
                                        <VBox prefWidth="562.0">
                                            <children>
                                                <FlowPane>
                                                    <children>
                                          <JFXComboBox fx:id="idDropDown" prefWidth="150.0" promptText="Edge">
                                             <FlowPane.margin>
                                                <Insets left="20.0" />
                                             </FlowPane.margin>
                                          </JFXComboBox>
                                          <JFXComboBox fx:id="startEdge" prefWidth="150.0" promptText="Edge Start Node">
                                             <FlowPane.margin>
                                                <Insets left="20.0" />
                                             </FlowPane.margin>
                                          </JFXComboBox>
                                          <JFXComboBox fx:id="endEdge" prefWidth="150.0" promptText="Edge End Node">
                                             <FlowPane.margin>
                                                <Insets left="20.0" />
                                             </FlowPane.margin>
                                          </JFXComboBox>
                                                    </children>
                                                    <VBox.margin>
                                                        <Insets top="20.0" />
                                                    </VBox.margin>
                                                </FlowPane>
                                                <FlowPane prefHeight="41.0" prefWidth="297.0">
                                                    <children>
                                                        <Button mnemonicParsing="false" onAction="#addEdgeButton" text="Add Edge">
                                                            <FlowPane.margin>
                                                                <Insets left="20.0" right="10.0" top="10.0" />
                                                            </FlowPane.margin>
                                                        </Button>
                                                        <Button mnemonicParsing="false" onAction="#editEdgeButton" text="Edit Edge">
                                                            <FlowPane.margin>
                                                                <Insets right="10.0" top="10.0" />
                                                            </FlowPane.margin>
                                                        </Button>
                                                        <Button mnemonicParsing="false" onAction="#deleteEdgeButton" text="Delete Edge">
                                                            <FlowPane.margin>
                                                                <Insets top="10.0" />
                                                            </FlowPane.margin>
                                                        </Button>
                                                    </children>
                                                    <VBox.margin>
                                                        <Insets top="20.0" />
                                                    </VBox.margin>
                                                </FlowPane>
                                            </children>
                                        </VBox>
                                        <StackPane prefWidth="611.0">
                                            <children>
                                                <ImageView fx:id="imageView" fitHeight="470.0" fitWidth="611.0" pickOnBounds="true" preserveRatio="true" />
                                                <Pane fx:id="pane" prefHeight="200.0" prefWidth="200.0" />
                                            </children>
                                        </StackPane>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                </HBox>
            </children>
        </AnchorPane>
        <AnchorPane prefHeight="58.0" prefWidth="1200.0">
            <children>
                <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <JFXButton fx:id="backButton" onAction="#toNavigation" prefHeight="29.0" prefWidth="60.0" style="-fx-background-color: -fx--primary-dark; -fx-border-color: -fx--primary-light;" styleClass="submit-button" text="Back">
                            <GridPane.margin>
                                <Insets bottom="20.0" left="20.0" />
                            </GridPane.margin>
                        </JFXButton>
                        <Button alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#startTableButton" text="Refresh" GridPane.columnIndex="1" GridPane.halignment="RIGHT">
                            <cursor>
                                <Cursor fx:constant="DEFAULT" />
                            </cursor>
                            <GridPane.margin>
                                <Insets bottom="20.0" right="20.0" />
                            </GridPane.margin>
                        </Button>
                    </children>
                </GridPane>
            </children>
        </AnchorPane>
    </children>
</VBox>
